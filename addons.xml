<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="skin.nofussconfluence"
       name="No Fuss Confluence Skin"
       version="0.1"
       provider-name="NoFussComputing">
    <requires>
         <import addon="xbmc.gui" version="5.3.0"/>
    </requires>
    <extension
        point="xbmc.gui.skin"
        defaultthemename="textures.xbt"
        debugging="false"
        effectslowdown="0.75">
        <res width="1280" height="720" aspect="16:9" default="true" folder="720p" />
    </extension>
</addon>
<addon id="repository.nofusscomputing" name="No Fuss Computings Addons" version="0.1" provider-name="NoFussComputing">
	<extension point="xbmc.addon.repository" name="No Fuss Computings Addons">
		<info compressed="false">http://raw.github.com/NoFussComputing/nofussrepository/master/addons.xml</info>
		<checksum>http://raw.github.com/NoFussComputing/nofussrepository/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/NoFussComputing/nofussrepository/master/addons</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>XBMC Addons provided by No Fuss Computing</summary>
		<description>This repository contains addons that were created by No Fuss Computing. http://nofusscomputing.com.</description>
		<disclaimer>-</disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>